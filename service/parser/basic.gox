package parser;

{% eachltr $size @sizes %}
func ParseUint{{ $size }}(data []byte) uint{{ $size }} {
	result :=
		{% define $id 1 %}
		{% define $upperBound (@diff (@log2 $size) 2) %} 
		{% whilertl ($id < $upperBound) %} 
			(uint{{ $size }}(data[{{ $id }}] << {{ @div $size 2 }}))
		{% endwhilertl %} 
		uint{{ $size }}(data[0]);
	
	return result;
}
{% endeachltr %}